# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
#end_of_line = crlf - Messes up a few things, including files we don't want to change
insert_final_newline = true
trim_trailing_whitespace = true
tab_width = 4

[*.java]
indent_style = space
indent_size = 4
max_line_length = 120